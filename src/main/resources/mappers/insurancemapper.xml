<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "https://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.solvd.hospital.dao.IInsuranceDao">
    <sql id="insuranceBody">
        Insurance.id as insurance_id,
        Insurance.name as insurance_name
    </sql>
    <resultMap id="insuranceResultMap" type="com.solvd.hospital.model.Insurance">
        <id property="id" column="insurance_id"/>
        <result property="name" column="insurance_name"/>
    </resultMap>
</mapper>